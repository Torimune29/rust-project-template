# Generated by Home Manager
root=true

[*]
charset=utf-8
end_of_line=lf
indent_size=2
indent_style=space
insert_final_newline=true
max_line_length=120
trim_trailing_whitespace=true

[*.go]
indent_size=4
indent_style=tab

[*.rs]
indent_size=4
max_line_length=99

[*.sh]
shell_variant=bash
switch_case_indent=true

[*.{markdown,md}]
indent_size=4
trim_trailing_whitespace=false

[*.{py,py.tpl}]
indent_size=4

[.git*]
indent_size=4
indent_style=tab

[LICENSE]
insert_final_newline=false

[{Make,Docker,Earth}File]
indent_size=4
indent_style=tab

